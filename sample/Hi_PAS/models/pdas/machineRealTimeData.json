    "SC_No":      {"type": "integer"},
    "DrivingMotor": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {"current": { "type": "number" }}
      }
    },
    "HoistingMotor": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {"current": { "type": "number" }}
      }
    },
    "ForkMotor": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {"current": { "type": "number" }}
      }
    }
  }
}