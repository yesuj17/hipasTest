div.container(style='height: 100%; width:100%; margin:0; padding:0')
    div#wems_top_content(style='height: 6%; width:100%; border:2px #f99 solid')
    div#wems_center_content(style='height: 47%; width:100%; border:2px #f99 solid')
        div#wems_cLeft_content(style='height: 100%; width:50%; float:left; border:2px #f99 solid')
        div#wems_cRight_content(style='height: 100%; width:50%; float:right; border:2px #f99 solid')
    div#wems_bottom_content(style='height: 47%; width:100%; border:2px #f99 solid')
        div#wems_bLeft_content(style='height: 100%; width:90%; float:left; border:2px #f99 solid')
        div#wems_bRight_content(style='height: 100%; width:10%; float:right; border:2px #f99 solid')
            div(style='height: 85%; width:100%; float:right; border:2px #f99 solid')
            div(style='height: 15%; width:100%; float:right; border:2px #f99 solid; text-align: center')
                button.btn.btn-primary.btn-md(type='button', data-toggle='modal', data-target='#wemsDetailModal' data-backdrop="static" data-keyboard="false") #{__('detail')}
                include wems/wems_detail_layout.jade
